    "@knorm/postgres": "file:packages/postgres",
    "@knorm/relations": "file:packages/relations",
    "@knorm/soft-delete": "file:packages/soft-delete",
    "@knorm/timestamps": "file:packages/timestamps",
    "@knorm/to-json": "file:packages/to-json",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.4.3",
    "@semantic-release/npm": "5.1.15",
