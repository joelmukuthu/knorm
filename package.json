  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "todo": "grep -rn -e TODO -e FIXME .",
    "jsdoc:knorm": "jsdoc2md --files packages/knorm/lib/**/*.js --partial docs/.jsdoc2md/templates/link.hbs --partial docs/.jsdoc2md/templates/body.hbs > packages/knorm/api.md",
    "jsdoc": "npm run jsdoc:knorm",
    "docs": "npm run jsdoc && vuepress dev docs",
    "docs:build": "npm run jsdoc && vuepress build docs",
    "mocha": "mocha",
    "nyc": "nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "docker-compose run --rm tests",
    "coverage": "docker-compose run --rm tests sh -c 'npm run nyc'"
  },
  "devDependencies": {
    "@knorm/field-name-to-column-name": "file:packages/field-name-to-column-name",
    "@knorm/knorm": "file:packages/knorm",
